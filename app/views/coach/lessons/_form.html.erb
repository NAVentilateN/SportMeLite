<%= simple_form_for(@lesson, method: :post, url: :coach_lessons, id: 'new_lesson_form', remote: true, data: { controller: 'post-form' }) do |f|%>
  <%= f.input :start_date_time, as: :datetime, html5: true %>
  <%= f.input :end_date_time, as: :datetime, html5: true %>
  <%= f.input :description %>
  <%= f.input :location %>
  <%= f.input :price, as: :numeric %>
  <%= f.submit class:'btn btn-primary' %>
<% end %>
